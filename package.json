{
  "name": "@ivosabev/helpers",
  "version": "0.53.1",
  "description": "Reusable Helpers",
  "keywords": [
    "javascript",
    "typescript",
    "ecmascript",
    "helpers",
    "utility functions"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivosabev/helpers.git"
  },
  "author": "Ivo Sabev (http://ivosabev.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ivosabev/helpers/issues"
  },
  "homepage": "https://github.com/ivosabev/helpers#readme",
  "engines": {
    "node": ">=18"
  },
  "type": "module",
  "sideEffects": false,
  "exports": {
    "./*": {
      "require": "./*.cjs",
      "import": "./*.js"
    }
  },
  "dependencies": {
    "@web3-storage/multipart-parser": "1.0.0",
    "chardet": "2.0.0",
    "crypto-js": "4.2.0",
    "fuzzysort": "3.0.2",
    "lodash": "4.17.21",
    "luxon": "3.5.0",
    "object-hash": "3.0.0",
    "papaparse": "5.4.1",
    "pluralize": "8.0.0",
    "validator": "13.12.0"
  },
  "peerDependencies": {
    "@graphql-tools/utils": "^10.x",
    "graphql": "^16.x",
    "graphql-relay": "^0.10.x",
    "react": "^18.x || ^19.x",
    "zod": "^3.x"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "devDependencies": {
    "@graphql-tools/utils": "10.5.4",
    "@ivosabev/eslint-config": "1.5.2",
    "@types/crypto-js": "4.2.2",
    "@types/lodash": "4.17.7",
    "@types/luxon": "3.4.2",
    "@types/node": "22.4.2",
    "@types/object-hash": "3.0.6",
    "@types/papaparse": "5.3.14",
    "@types/pluralize": "0.0.33",
    "@types/react": "18.3.4",
    "@types/validator": "13.12.0",
    "@typescript-eslint/parser": "8.2.0",
    "esbuild": "0.23.1",
    "eslint": "8.57.0",
    "graphql": "16.9.0",
    "graphql-relay": "0.10.2",
    "npm-check": "6.0.1",
    "prettier": "3.3.3",
    "react": "18.3.1",
    "release-it": "17.6.0",
    "tslib": "2.6.3",
    "tsup": "8.2.4",
    "typescript": "5.5.4",
    "zod": "3.23.8"
  },
  "scripts": {
    "build": "rm -rf dist && tsup && scripts/afterBuild.sh",
    "lint:check": "eslint src/**/*.ts",
    "lint": "eslint src/**/*.ts --fix",
    "prettier:check": "prettier . --check",
    "prettier": "prettier . --write",
    "release": "npm run build && npx release-it",
    "up": "npm-check --update -f",
    "watch": "npm run build && watchman-make -p 'src/**/*' --run 'npm run build'"
  }
}
