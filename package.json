{
  "name": "@ivosabev/helpers",
  "version": "0.46.1",
  "description": "Reusable Helpers",
  "keywords": [
    "javascript",
    "typescript",
    "ecmascript",
    "helpers",
    "utility functions"
  ],
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "jsnext:main": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivosabev/helpers.git"
  },
  "author": "Ivo Sabev (http://ivosabev.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ivosabev/helpers/issues"
  },
  "homepage": "https://github.com/ivosabev/helpers#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@web3-storage/multipart-parser": "1.0.0",
    "crypto-js": "4.2.0",
    "fuzzysort": "2.0.4",
    "lodash": "4.17.21",
    "luxon": "3.4.4",
    "object-hash": "3.0.0",
    "pluralize": "8.0.0",
    "validator": "13.11.0"
  },
  "peerDependencies": {
    "@graphql-tools/utils": "10.x",
    "graphql": "^16.x",
    "graphql-relay": "0.10.0",
    "react": "18.x",
    "zod": "3.x"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "devDependencies": {
    "@graphql-tools/utils": "10.1.0",
    "@ivosabev/eslint-config": "1.3.3",
    "@rollup/plugin-commonjs": "25.0.7",
    "@rollup/plugin-json": "6.1.0",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@rollup/plugin-typescript": "11.1.6",
    "@types/crypto-js": "4.2.2",
    "@types/lodash": "4.14.202",
    "@types/luxon": "3.4.2",
    "@types/node": "20.11.24",
    "@types/object-hash": "3.0.6",
    "@types/pluralize": "0.0.33",
    "@types/react": "18.2.61",
    "@types/validator": "13.11.9",
    "esbuild": "0.20.1",
    "eslint": "8.57.0",
    "graphql": "16.8.1",
    "graphql-relay": "0.10.0",
    "npm-check": "6.0.1",
    "prettier": "3.2.5",
    "react": "18.2.0",
    "release-it": "17.1.1",
    "rollup": "4.12.0",
    "rollup-plugin-dts": "6.1.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-sourcemaps": "0.6.3",
    "tslib": "2.6.2",
    "typescript": "5.3.3",
    "zod": "3.22.4"
  },
  "scripts": {
    "build-cjs-types": "tsc --emitDeclarationOnly --outDir \"./dist/cjs\"",
    "build-esm-types": "tsc --emitDeclarationOnly --outDir \"./dist/esm\"",
    "build": "npm run clean && rollup -c rollup.config.mjs && scripts/afterBuild.sh",
    "clean": "rm -rf ./dist && mkdir dist",
    "compile:dist": "tsc --project tsconfig.dist.json",
    "compile": "tsc",
    "lint:check": "eslint src/**/*.ts",
    "lint": "eslint src/**/*.ts --fix",
    "prettier:check": "prettier . --check",
    "prettier": "prettier . --write",
    "release": "npm run build && npx release-it",
    "up": "npm-check --update -f",
    "watch": "npm run build && watchman-make -p 'src/**/*' --run 'npm run build'"
  }
}
