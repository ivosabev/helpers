{
  "name": "@ivosabev/helpers",
  "version": "0.73.0",
  "description": "Reusable Helpers",
  "keywords": [
    "javascript",
    "typescript",
    "ecmascript",
    "helpers",
    "utility functions"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivosabev/helpers.git"
  },
  "author": "Ivo Sabev (http://ivosabev.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ivosabev/helpers/issues"
  },
  "homepage": "https://github.com/ivosabev/helpers#readme",
  "engines": {
    "node": ">=18"
  },
  "type": "module",
  "sideEffects": false,
  "exports": {
    "./*": {
      "require": "./*.cjs",
      "import": "./*.js"
    }
  },
  "dependencies": {
    "@web3-storage/multipart-parser": "1.0.0",
    "chardet": "2.1.0",
    "crypto-js": "4.2.0",
    "fuzzysort": "3.1.0",
    "lodash": "4.17.21",
    "luxon": "3.7.2",
    "object-hash": "3.0.0",
    "papaparse": "5.5.3",
    "pluralize": "8.0.0",
    "type-fest": "5.0.1",
    "validator": "13.15.15"
  },
  "peerDependencies": {
    "@graphql-tools/utils": "10.x",
    "graphql": "16.x",
    "graphql-relay": "0.10",
    "react": "18.x || 19.x",
    "zod": "3.x"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "devDependencies": {
    "@graphql-tools/utils": "10.9.1",
    "@ivosabev/config": "2.10.0",
    "@types/crypto-js": "4.2.2",
    "@types/lodash": "4.17.20",
    "@types/luxon": "3.7.1",
    "@types/node": "24.5.2",
    "@types/object-hash": "3.0.6",
    "@types/papaparse": "5.3.16",
    "@types/pluralize": "0.0.33",
    "@types/react": "19.1.13",
    "@types/validator": "13.15.3",
    "esbuild": "0.25.10",
    "eslint": "9.36.0",
    "graphql": "16.11.0",
    "graphql-relay": "0.10.2",
    "npm-run-all": "4.1.5",
    "react": "19.1.1",
    "release-it": "19.0.5",
    "tslib": "2.8.1",
    "tsup": "8.5.0",
    "typescript": "5.9.2",
    "zod": "4.1.11"
  },
  "scripts": {
    "build": "npm run validate && rm -rf dist && tsup && scripts/afterBuild.sh",
    "lint-check": "eslint .",
    "lint": "eslint . --fix",
    "release-it": "release-it",
    "release": "scripts/release.sh",
    "typecheck": "tsc",
    "up": "pnpm update --interactive --latest",
    "validate": "run-p -l lint typecheck",
    "watch": "npm run build && watchman-make -p 'src/**/*' --run 'npm run build'"
  },
  "packageManager": "pnpm@9.15.9+sha512.68046141893c66fad01c079231128e9afb89ef87e2691d69e4d40eee228988295fd4682181bae55b58418c3a253bde65a505ec7c5f9403ece5cc3cd37dcf2531"
}
