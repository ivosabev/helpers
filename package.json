{
  "name": "@ivosabev/helpers",
  "version": "0.69.0",
  "description": "Reusable Helpers",
  "keywords": [
    "javascript",
    "typescript",
    "ecmascript",
    "helpers",
    "utility functions"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivosabev/helpers.git"
  },
  "author": "Ivo Sabev (http://ivosabev.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ivosabev/helpers/issues"
  },
  "homepage": "https://github.com/ivosabev/helpers#readme",
  "engines": {
    "node": ">=18"
  },
  "type": "module",
  "sideEffects": false,
  "exports": {
    "./*": {
      "require": "./*.cjs",
      "import": "./*.js"
    }
  },
  "dependencies": {
    "@web3-storage/multipart-parser": "1.0.0",
    "chardet": "2.1.0",
    "crypto-js": "4.2.0",
    "fuzzysort": "3.1.0",
    "lodash": "4.17.21",
    "luxon": "3.6.1",
    "object-hash": "3.0.0",
    "papaparse": "5.5.2",
    "pluralize": "8.0.0",
    "validator": "13.15.0"
  },
  "peerDependencies": {
    "@graphql-tools/utils": "10.x",
    "graphql": "16.x",
    "graphql-relay": "0.10",
    "react": "18.x || 19.x",
    "zod": "3.x"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "devDependencies": {
    "@graphql-tools/utils": "10.8.6",
    "@ivosabev/config": "2.9.12",
    "@types/crypto-js": "4.2.2",
    "@types/lodash": "4.17.16",
    "@types/luxon": "3.6.2",
    "@types/node": "22.15.17",
    "@types/object-hash": "3.0.6",
    "@types/papaparse": "5.3.16",
    "@types/pluralize": "0.0.33",
    "@types/react": "19.1.4",
    "@types/validator": "13.15.0",
    "esbuild": "0.25.4",
    "eslint": "9.26.0",
    "graphql": "16.11.0",
    "graphql-relay": "0.10.2",
    "npm-check": "6.0.1",
    "npm-run-all": "4.1.5",
    "react": "19.1.0",
    "release-it": "19.0.2",
    "tslib": "2.8.1",
    "tsup": "8.4.0",
    "typescript": "5.8.3",
    "zod": "3.24.4"
  },
  "scripts": {
    "build": "npm run validate && rm -rf dist && tsup && scripts/afterBuild.sh",
    "lint-check": "eslint .",
    "lint": "eslint . --fix",
    "release-it": "release-it",
    "release": "scripts/release.sh",
    "typecheck": "tsc",
    "up": "npm-check --update -f",
    "validate": "run-p -l lint typecheck",
    "watch": "npm run build && watchman-make -p 'src/**/*' --run 'npm run build'"
  },
  "packageManager": "pnpm@9.12.2+sha512.22721b3a11f81661ae1ec68ce1a7b879425a1ca5b991c975b074ac220b187ce56c708fe5db69f4c962c989452eee76c82877f4ee80f474cebd61ee13461b6228"
}
